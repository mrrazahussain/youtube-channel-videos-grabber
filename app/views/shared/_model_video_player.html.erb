<div id="videoModal<%= video.video_id%>" class="reveal-modal medium" data-reveal aria-labelledby="videoModalTitle" aria-hidden="true" role="dialog">
  <!--<h2 id="videoModalTitle"><%#= video.title%></h2>-->
  <div class="flex-video widescreen vimeo">
    <iframe width="1280" height="720" src="//www.youtube-nocookie.com/embed/<%= video.video_id%>?rel=0" frameborder="0" allowfullscreen></iframe>
  </div>

  <a class="close-reveal-modal" aria-label="Close">&#215;</a>
</div>